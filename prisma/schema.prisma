generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Appointment {
  id          String   @id @default(cuid())
  tutorName   String
  petName     String
  phone       String
  description String
  scheduleAt  DateTime

  @@map("appointments")
}
